	<queryString language="SQL">
		<![CDATA[SELECT e.product_code,
                   i.product_name,
                   e.product_color,
                   get_comm(e.product_size) as product_size,
                   e.lot,
                   e.process_date,
                   e.error_charger,
                   e.error_info
